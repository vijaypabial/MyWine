<h1>Wines</h1>
<%= link_to "Create new Wine", new_wine_path%>

 <% @wines.each do |wine|%>
     <p><%= wine.title%></p>
     <p><%= image_tag("download.jpeg")%></p>
     <p><%= wine.variety%></p>
     <p><%= wine.description%></p>
     <p><%= wine.price%></p>
     <%= link_to "Edit", edit_wine_path(wine.id)%>
    <%end%>